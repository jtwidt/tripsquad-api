{
  "name": "tripsquad-api",
  "version": "1.0.0",
  "description": "The API for the TripSquad application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:init": "npx sequelize-cli db:create && npx sequelize-cli db:migrate",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jtwidt/tripsquad-api.git"
  },
  "author": "Jeremy Twidt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jtwidt/tripsquad-api/issues"
  },
  "homepage": "https://github.com/jtwidt/tripsquad-api#readme",
  "dependencies": {
    "@clerk/clerk-sdk-node": "^5.0.42",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "pg": "^8.13.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.6",
    "sequelize-cli": "^6.6.2"
  }
}
